{
  "name": "livelyicons",
  "version": "1.1.0",
  "description": "A comprehensive library of animated icons powered by Lively for React",
  "homepage": "https://livelyicons.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livelyicons/icons.git"
  },
  "bugs": {
    "url": "https://github.com/livelyicons/icons/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "livelyicons": "./bin/livelyicons.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./static": {
      "types": "./dist/static.d.ts",
      "import": "./dist/static.mjs",
      "require": "./dist/static.js"
    },
    "./css": {
      "types": "./dist/css.d.ts",
      "import": "./dist/css.mjs",
      "require": "./dist/css.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "src/icons",
    "src/static",
    ".vscode",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsup",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate:types": "npx tsx scripts/generate-types.ts",
    "benchmark": "npx tsx scripts/benchmark-bundle-size.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run generate:types && pnpm run build:lib"
  },
  "keywords": [
    "react",
    "icons",
    "motion",
    "animation",
    "components",
    "animated-icons",
    "framer-motion",
    "svg-icons",
    "rsc",
    "server-components",
    "css-animations"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "motion": ">=11.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "motion": {
      "optional": true
    }
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.3",
    "@monaco-editor/react": "^4.7.0",
    "@react-email/components": "^1.0.7",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.2.0",
    "@vercel/postgres": "^0.10.0",
    "dompurify": "^3.3.1",
    "drizzle-orm": "^0.45.1",
    "fuse.js": "^7.1.0",
    "inngest": "^3.52.0",
    "jszip": "^3.10.1",
    "modern-gif": "^2.0.4",
    "next": "^15.0.0",
    "resend": "^6.9.1",
    "stripe": "^20.3.1",
    "svix": "^1.84.1",
    "tailwindcss": "^4.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^11.0.0",
    "drizzle-kit": "^0.31.9",
    "esbuild": "^0.24.2",
    "jsdom": "^28.0.0",
    "motion": "^12.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
